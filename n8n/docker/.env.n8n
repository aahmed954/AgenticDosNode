# N8N Configuration
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=secure_n8n_password_change_me
N8N_HOST=localhost
N8N_PROTOCOL=http
N8N_WEBHOOK_URL=http://localhost:5678/
DOMAIN=localhost
TIMEZONE=UTC

# Database Configuration
N8N_POSTGRES_PASSWORD=n8n_secure_db_password_change_me
N8N_POSTGRES_USER_PASSWORD=n8n_user_password_change_me
REDIS_PASSWORD=redis_secure_password_change_me

# AI Service Integration
CLAUDE_API_KEY=your_claude_api_key_here
CLAUDE_PROXY_URL=http://ccproxy:3000
OPENAI_API_KEY=your_openai_api_key_here
VECTOR_DB_URL=http://qdrant:6333
REDIS_URL=redis://redis-n8n:6379

# External Service APIs
GITHUB_TOKEN=your_github_token_here
SLACK_BOT_TOKEN=xoxb-your-slack-bot-token-here
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/SLACK/WEBHOOK
NEWS_API_KEY=your_news_api_key_here
WEATHER_API_KEY=your_weather_api_key_here

# Email Configuration
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASSWORD=your_app_password_here
EMAIL_FROM=n8n-automation@yourdomain.com

# Security
JWT_SECRET=generate_random_jwt_secret_here
ENCRYPTION_KEY=generate_random_32_char_encryption_key

# Monitoring
PROMETHEUS_ENABLED=true
GRAFANA_ENABLED=true

# Backup Configuration
BACKUP_SCHEDULE="0 2 * * *"  # Daily at 2 AM
BACKUP_RETENTION_DAYS=30
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
BACKUP_S3_BUCKET=n8n-backups-bucket